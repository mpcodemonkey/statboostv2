<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.statboost.models.email.GroupEventLink" table="stt_group_event_link" schema="" catalog="statboost">
        <id name="uid" column="gel_uid"/>
        <many-to-one name="emailVariableGroup" class="com.statboost.models.email.EmailVariableGroup"
                     column="gel_evg_uid" not-null="true" insert="true" update="true"
                     cascade="all" />
        <many-to-one name="workflowEvent" class="com.statboost.models.email.WorkflowEvent"
                     column="gel_wev_uid" not-null="true" insert="true" update="true"
                     cascade="all" />
    </class>
</hibernate-mapping>