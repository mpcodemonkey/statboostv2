<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.statboost.models.email.EmailTemplate" table="stt_email_template">
        <id name="uid" column="etm_uid"/>
        <property name="name" column="etm_name"/>
        <property name="body" column="etm_body"/>
    </class>
</hibernate-mapping>