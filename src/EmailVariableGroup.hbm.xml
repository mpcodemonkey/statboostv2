<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.statboost.models.email.EmailVariableGroup" table="stt_email_variable_group">
        <id name="uid" column="evg_uid">
            <generator class="identity"/>
        </id>
        <property name="name" column="evg_name"/>
    </class>
</hibernate-mapping>