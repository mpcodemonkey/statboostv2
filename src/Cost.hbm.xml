<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.statboost.models.inventory.Cost" table="stt_cost">
        <id name="cstUid" column="cst_uid"/>
        <property name="cstItemCondition" column="cst_item_condition"/>
        <property name="cstItemPrice" column="cst_item_price"/>
        <property name="cstItemQuantity" column="cst_item_quantity"/>
        <property name="cstInvUid" column="cst_inv_uid"/>

        <many-to-one name="inventory"
                     column="cst_inv_uid"
                     not-null="true" insert="false" update="false"/>
    </class>
</hibernate-mapping>