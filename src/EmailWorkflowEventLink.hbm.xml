<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.statboost.models.email.EmailWorkflowEventLink" table="stt_email_workflow_event_link">
        <id name="uid" column="ewe_uid"/>
        <many-to-one name="email" class="com.statboost.models.email.Email"
                     column="ewe_eml_uid" not-null="true" insert="true" update="true"
                     cascade="all" />
        <many-to-one name="emailWorkflowEvent" class="com.statboost.models.email.WorkflowEvent"
                     column="ewe_wev_uid" not-null="true" insert="true" update="true"
                     cascade="all" />
    </class>
</hibernate-mapping>