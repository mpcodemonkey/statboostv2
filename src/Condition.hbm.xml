<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.statboost.models.inventory.Condition" table="stt_condition" schema="" catalog="statboost">
        <id name="uid" column="cnd_uid"/>
        <property name="name" column="cnd_name"/>
        <property name="percentOfPrice" column="cnd_percent_of_price"/>
    </class>
</hibernate-mapping>