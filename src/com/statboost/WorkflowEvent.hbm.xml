<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.statboost.WorkflowEvent" table="stt_workflow_event" schema="" catalog="statboost">
        <id name="uid" column="wev_uid"/>
        <property name="name" column="wev_name"/>
        <property name="description" column="wev_description"/>
        <property name="code" column="wev_code"/>
    </class>
</hibernate-mapping>