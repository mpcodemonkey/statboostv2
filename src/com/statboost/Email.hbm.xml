<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.statboost.Email" table="stt_email">
        <id name="uid" type="int" column="eml_uid">
            <generator class="native"/>
        </id>
        <property name="name" column="eml_name" type="string"/>
        <property name="subject" column="eml_subject" type="string"/>
        <property name="from" column="eml_from" type="string"/>
        <property name="to" column="eml_to" type="string"/>
        <property name="body" column="eml_body" type="string"/>
        <property name="emailTemplateUid" column="eml_etm_uid" type="integer"/>
        <property name="emailVariableGroupUid" column="eml_vgr_uid" type="integer"/>
    </class>
</hibernate-mapping>