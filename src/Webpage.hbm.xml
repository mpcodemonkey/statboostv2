<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.statboost.models.form.Webpage" table="stt_webpage">
        <id name="uid" column="wbp_uid"/>
        <property name="name" column="wbp_name"/>
        <property name="body" column="wbp_body"/>
        <property name="code" column="wbp_code"/>
    </class>
</hibernate-mapping>