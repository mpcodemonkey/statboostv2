<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.statboost.models.inventory.ConditionInventoryLink" table="stt_condition_inventory_link" schema=""
           catalog="statboost">
        <id name="uid" column="cil_uid"/>
        <property name="numberInStock" column="cil_number_in_stock"/>
        <many-to-one name="inventory" class="com.statboost.models.inventory.Inventory"
                     column="cil_inv_uid" not-null="true" insert="true" update="true"
                     cascade="all" />
        <many-to-one name="condition" class="com.statboost.models.inventory.Condition"
                     column="cil_cnd_uid" not-null="true" insert="true" update="true"
                     cascade="all" />
    </class>
</hibernate-mapping>